generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produto {
  id        Int      @id @default(autoincrement())
  name      String
  bar_code  String   @unique
  price     Decimal
  create_at DateTime @default(now())

  @@map("products")
}

