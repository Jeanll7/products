generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produto {
  id     Int      @id @default(autoincrement())
  nome   String
  codigo String   @unique
  preco  Decimal
  data   DateTime @default(now())

  @@map("produtos")
}
